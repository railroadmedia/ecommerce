{
  "name": "railroad/ecommerce",
  "type": "library",
  "description": "Ecommerce system",
  "repositories": [
    {
      "type": "path",
      "url": "../location"
    },
    {
      "type": "path",
      "url": "../permissions"
    },
    {
      "type": "path",
      "url": "../remotestorage"
    }
  ],
  "require": {
    "php": "~7",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
    "doctrine/dbal": "^2.5",
    "ramsey/uuid": "^3.6",
    "webpatser/laravel-countries": "1.5.4",
    "stripe/stripe-php": "^4.4",
    "railroad/location": "^1.0",
    "railroad/permissions": "^1.0",
    "railroad/remotestorage": "^1.0",
    "railroad/resora": "^1.0",
    "railroad/response": "^1.0",
    "railroad/usora": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "phpunit/php-code-coverage": "6.0.1",
    "orchestra/database": "3.6.x@dev",
    "orchestra/testbench": "~3.6",
    "filp/whoops": "~2.1",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Ecommerce\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Ecommerce\\Tests\\": "tests/"
    }
  }
}
