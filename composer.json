{
  "name": "railroad/ecommerce",
  "type": "library",
  "description": "Ecommerce system",
  "require": {
    "php": "~7.1",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
    "doctrine/dbal": "^2.5",
    "ramsey/uuid": "^3.6",
    "stripe/stripe-php": "^4.4",
    "railroad/location": "dev-v2.0-",
    "railroad/permissions": "^1.0",
    "railroad/remotestorage": "^1.0",
    "railroad/resora": ">=1.0.10 <1.1",
    "railroad/doctrine": "^0.0",
    "ext-redis": "*",
    "railroad/doctrine-array-hydrator": "1.0.*",
    "ext-json": "*",
    "aporat/store-receipt-validator": "3.2.*",
    "railroad/railactionlog": "1.0.*",
    "tymon/jwt-auth": "1.0.0-rc.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "phpunit/php-code-coverage": "6.0.1",
    "orchestra/database": "3.6.x@dev",
    "orchestra/testbench": "~3.6",
    "filp/whoops": "~2.1",
    "mockery/mockery": "^1.0",
    "brianium/paratest": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Ecommerce\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Ecommerce\\Tests\\": "tests/",
      "Railroad\\Ecommerce\\Seeds\\": "seeds/"
    }
  }
}
