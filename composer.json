{
    "name": "railroad/ecommerce",
    "type": "library",
    "description": "Ecommerce system",
    "minimum-stability": "dev",
 "require": {
    "php": "~7",
    "laravel/framework": "5.5.*",
    "guzzlehttp/guzzle": "~6.0",
    "doctrine/dbal": "^2.5",
    "ramsey/uuid": "^3.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "phpunit/php-code-coverage": "5.2.2",
    "orchestra/database": "3.5.x@dev",
    "orchestra/testbench": "~3.4",
    "filp/whoops":"~2.1"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Ecommerce\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Ecommerce\\Tests\\": "tests/"
    }
  }
}
