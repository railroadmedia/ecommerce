{
  "name": "railroad/ecommerce",
  "type": "library",
  "description": "Ecommerce system",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "guzzlehttp/guzzle": "^7.2",
    "doctrine/dbal": "^3.3",
    "ramsey/uuid": "^4.3",
    "railroad/location": "^3.0.0",
    "railroad/permissions": "^2.0",
    "railroad/remotestorage": "^2.0",
    "railroad/doctrine": "2.0.*",
    "railroad/doctrine-array-hydrator": "2.0.*",
    "ext-redis": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "aporat/store-receipt-validator": "4.2.*",
    "stripe/stripe-php": "^7.95",
    "ext-pdo": "*",
    "railroad/referral": "3.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Ecommerce\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Ecommerce\\Tests\\": "tests/",
      "Railroad\\Ecommerce\\Seeds\\": "seeds/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "composer/package-versions-deprecated": true
    }
  }
}
