{
    "name": "railroad/ecommerce",
    "type": "library",
    "description": "Ecommerce system",
    "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "../location"
    }
  ],
 "require": {
    "php": "~7",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
    "doctrine/dbal": "^2.5",
    "ramsey/uuid": "^3.6",
    "railroad/location" : "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "phpunit/php-code-coverage": "6.0.1",
    "orchestra/database": "3.6.x@dev",
    "orchestra/testbench": "~3.6",
    "filp/whoops":"~2.1"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Ecommerce\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Ecommerce\\Tests\\": "tests/"
    }
  }
}
